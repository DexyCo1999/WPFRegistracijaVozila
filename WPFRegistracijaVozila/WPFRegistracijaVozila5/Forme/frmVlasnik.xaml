        <TextBox Name="txtGradVlasnika" HorizontalAlignment="Left" Height="23" Margin="123,148,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="txtKontaktVlasnika" HorizontalAlignment="Left" Height="23" Margin="123,178,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Button Name="btnSacuvaj" Content="Sačuvaj" HorizontalAlignment="Left" Margin="192,227,0,0" VerticalAlignment="Top" Width="75" Click="BtnSacuvaj_Click"/>
        <Button Name="btnOtkazi" Content="Otkaži" HorizontalAlignment="Left" Margin="58,227,0,0" VerticalAlignment="Top" Width="75" Click="BtnOtkazi_Click"/>

    </Grid>
</Window>
